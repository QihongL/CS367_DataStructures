import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;


public class FileSystemMain {

	private static Scanner scnr;
	private static File inputFile;


	public static void main(String[] args) {

		// TODO should we assume there is only one input command line argument?
		if(args.length == 1) {
			inputFile = new File(args[0]);
		} else {
			// TODO what to print out?
			System.out.println("NUM COMMAND LINE ARGS INVALID");
			System.exit(0);
		}

		// read the inputFile 
		try {
			scnr = new Scanner(inputFile);
		} catch (FileNotFoundException e) {
			//			 TODO what to print when File not found ? 
			e.printStackTrace();
		}

		
		
		scnr.nextLine();
		
		// process the inputFile, create the file system
		while(scnr.hasNext()){
			System.out.println(scnr.nextLine());
		}


	}	

}
