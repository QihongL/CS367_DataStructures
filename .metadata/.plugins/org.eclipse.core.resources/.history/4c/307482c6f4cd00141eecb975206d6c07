class BinaryTreenode<T> {
    // *** fields ***
    private T key;
    private BinaryTreenode<T> left, right;
 
    // *** constructor ***
    public BinaryTreenode(T key, BinaryTreenode<T> left, BinaryTreenode<T> right) {
        this.key = key;
        this.left = left;
        this.right = right;
    }
 
    // *** methods ***
    
    // accessors (access to fields)
    public T getKey() { return key; }
    public BinaryTreenode<T> getLeft() { return left; }
    public BinaryTreenode<T> getRight() { return right; }
 

    // mutators (change fields)
    public void setKey(T newK) { key = newK; }
    public void setLeft(BinaryTreenode<T> newL) { left = newL; }
    public void setRight(BinaryTreenode<T> newR) { right = newR; }
    
    public int countMatches(BinaryTreenode<T> tree, T item){
    	
    }
}